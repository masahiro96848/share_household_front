{
    "name": "share_household",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "lint": "next lint --dir src",
        "lint:fix": "eslint src --ext .js,jsx,.ts,.tsx --fix",
        "prepare": "husky install",
        "check-types": "tsc --pretty --noEmit",

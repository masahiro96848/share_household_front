{
    "compilerOptions": {
        "target": "es5",
        "lib": ["dom", "dom.iterable", "esnext"],
        "allowJs": true,
        "skipLibCheck": true,
        "strict": true,
        "forceConsistentCasingInFileNames": true,
        "noEmit": true,
        "esModuleInterop": true,
        "module": "esnext",
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "isolatedModules": true,
        "jsx": "preserve",
        "incremental": true,
        "baseUrl": "./src",
        "plugins": [
            {
                "name": "next"
            }
        ],
        "paths": {
            "@/*": ["*"]
        },
        "types": ["jest"]
    },
    "include": [
        "next-env.d.ts",
        "**/*.ts",
        "**/*.tsx",
        ".next/types/**/*.ts",
        "/Users/masahiro/Desktop/develop/output/share_household_front/share_household/.next/types/**/*.ts"
    ],
    "exclude": ["node_modules", "jest"]
}
